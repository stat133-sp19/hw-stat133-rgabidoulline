\name{bin_variance}
\alias{bin_variance}
\title{Binomial Variance}
\usage{
bin_variance(trials, prob)
}
\arguments{
\item{trials}{number of trials}

\item{prob}{probability of success}
}
\value{
variance of binomial distribution with given parameters
}
\description{
Computes the variance of a binomial distribution
}
\examples{
# variance of distribution with trials = 10 and prob = 0.3
bin_variance(10, 0.3)
}
